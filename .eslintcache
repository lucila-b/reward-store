{user?.points < props.cost}>\n                <ImageContainer>\n                    <Image src={props.img.url}/>\n                    {user?.points.cost ? <ShoppingIcon src={MainShoppingIcon}></ShoppingIcon> :\n                    <NotEnoughCoinsContainer>\n                        <NotEnoughAlert>You need {props.cost}</NotEnoughAlert>\n                        <Coin src={CoinIcon}></Coin>\n                    </NotEnoughCoinsContainer>}\n                </ImageContainer>\n                <Separator></Separator>\n                <NameCategoryTitle>\n                    <Category>{props.category}</Category>\n                    <Name>{props.name}</Name>\n                </NameCategoryTitle>\n                <HoverContainer>\n                    <ShoppingIconHover src={MainShoppingIconHov}></ShoppingIconHover>\n                    <CoinsContainer>\n                        <HoverSaldo>{props.cost}</HoverSaldo>\n                        <CoinHover src={CoinIcon}></CoinHover>\n                    </CoinsContainer>\n                    <RedeemButton onClick={() =>redeem(props._id, props.cost)}>Redeem now</RedeemButton>\n                </HoverContainer>\n            </MainContainerProduct>\n            <Modal\n                aria-labelledby=\"transition-modal-title\"\n                aria-describedby=\"transition-modal-description\"\n                className={classes.modal}\n                open={openRedeemModal}\n                onClose={ () => setOpenRedeemModal(false) }\n                closeAfterTransition\n                BackdropComponent={Backdrop}\n                BackdropProps={{\n                    timeout: 0\n                }}\n            >\n                <Fade in={openRedeemModal}>\n                    <div className={classes.paper}>\n                        <Title id=\"transition-modal-title\">\n                            {openRedeemModal\n                                ? 'Congratulations!'\n                                : 'Ops... something went wrong!'}\n                        </Title>\n                        <p id=\"transition-modal-description\">\n                            {openRedeemModal\n                                ? \"You've redeemed the product successfully\"\n                                : 'Please try again in a few minutes.'}\n                        </p>\n                    </div>\n                </Fade>\n           </Modal>\n        </> \n    )\n}","/Users/marinoraul/Sites/reward-store/src/components/Redeems/Redeems.js",[],"/Users/marinoraul/Sites/reward-store/src/components/RedeemedProduct.js",[],"/Users/marinoraul/Sites/reward-store/src/styles/theme.js",[],"/Users/marinoraul/Sites/reward-store/src/styles/PagesContainer.js",[],"/Users/marinoraul/Sites/reward-store/src/components/GetCoins/GetCoins.js",[],"/Users/marinoraul/Sites/reward-store/src/styles/Coins.js",[],{"ruleId":"77","replacedBy":"78"},{"ruleId":"79","replacedBy":"80"},{"ruleId":"81","severity":1,"message":"82","line":1,"column":17,"nodeType":"83","messageId":"84","endLine":1,"endColumn":27},{"ruleId":"81","severity":1,"message":"85","line":1,"column":29,"nodeType":"83","messageId":"84","endLine":1,"endColumn":38},{"ruleId":"81","severity":1,"message":"86","line":1,"column":8,"nodeType":"83","messageId":"84","endLine":1,"endColumn":13},{"ruleId":"87","severity":1,"message":"88","line":109,"column":11,"nodeType":"89","endLine":109,"endColumn":13,"suggestions":"90"},{"ruleId":"81","severity":1,"message":"91","line":5,"column":8,"nodeType":"83","messageId":"84","endLine":5,"endColumn":12},{"ruleId":"81","severity":1,"message":"92","line":6,"column":8,"nodeType":"83","messageId":"84","endLine":6,"endColumn":13},{"ruleId":"81","severity":1,"message":"93","line":106,"column":9,"nodeType":"83","messageId":"84","endLine":106,"endColumn":27},{"ruleId":"81","severity":1,"message":"94","line":109,"column":11,"nodeType":"83","messageId":"84","endLine":109,"endColumn":19},{"ruleId":"81","severity":1,"message":"95","line":118,"column":11,